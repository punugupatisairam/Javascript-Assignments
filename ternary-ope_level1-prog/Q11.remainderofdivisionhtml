//  Write a program to print the reminder of a division
     // Get user input for dividend and divisor
var dividend = parseInt(prompt("Enter the dividend:")); 
var divisor = parseInt(prompt("Enter the divisor:"));

if (divisor !== 0) {                         // Check if the divisor is not zero

  var remainder = dividend % divisor;          // Calculate the remainder

  // Print the result
  console.log(`The remainder of ${dividend} divided by ${divisor} is: ${remainder}`);
} else {
  console.log("Error: Division by zero is not allowed.");
}

    </script>
  
